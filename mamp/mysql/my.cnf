
[mysqladmin]
user=root

[mysqld]
basedir=/goinfre/myang/php/mamp/mysql
datadir="/goinfre/myang/php/mamp/mysql/data"
port=3306
socket=/goinfre/myang/php/mamp/mysql/tmp/mysql.sock
tmpdir=/goinfre/myang/php/mamp/mysql/tmp
max_allowed_packet=32M
bind-address=127.0.0.1
skip-name-resolve=1

character-set-server=UTF8
collation-server=utf8_general_ci
[client]
port=3306
socket=/goinfre/myang/php/mamp/mysql/tmp/mysql.sock

default-character-set=UTF8
[manager]
port=3306
socket=/goinfre/myang/php/mamp/mysql/tmp/mysql.sock
pid-file=/goinfre/myang/php/mamp/mysql/tmp/manager.pid
default-mysqld-path=/goinfre/myang/php/mamp/mysql/bin/mysqld.bin

